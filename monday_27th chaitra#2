<!DOCTYPE html>
<html>
<head>
	<title></title>
</head>
<body
>	<script>
		class Car{
			constructor(name, year){
				this.name = name;
				this.year = year;
			}
			display(){
				document.write("Name of the car: " + this.name + "<br />")
				document.write("Year manufactured: "+ this.year + "<br />")
			}
		}
		const myCar = new Car("Ford", 2018);
		myCar.display();
	</script>
</body>
</html>